    <!-- 手机振动权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- Required 自定义用来收发消息的相关权限 -->
    <permission
        android:name="com.ehelp.permission.JPUSH_MESSAGE"
